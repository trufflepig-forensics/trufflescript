{
  "name": "trufflescript",
  "version": "0.1.0",
  "packageManager": "yarn@3.3.1",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build"
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 4,
    "printWidth": 90,
    "bracketSameLine": false
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "prettier": "^2.8.8",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "files": [
    "dist",
    "src"
  ],
  "bundledDependencies": false,
  "module": "./dist/trufflescript.es.js",
  "main": "./dist/trufflescript.umd.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/trufflescript.es.js",
      "require": "./dist/trufflescript.umd.js",
      "types": "./dist/index.d.ts"
    }
  }
}
